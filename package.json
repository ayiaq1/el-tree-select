{
  "name": "el-tree-select",
  "version": "3.1.12",
  "description": "基于element-ui2.x扩展下拉带树的组件,目前已剥离组件可作为独立插件使用",
  "author": "dawdler",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ayiaq1/el-tree-select"
  },
  "bugs": {
    "url": "https://github.com/ayiaq1/el-tree-select/issues",
    "email": "250751717@qq.com"
  },
  "keywords": [
    "vue",
    "vue-cli3",
    "element-ui",
    "select",
    "tree"
  ],
  "scripts": {
    "api": "vuese gen --title=el-tree-select && rmdir /s/q docs\\website && move website docs",
    "api:open": "vuese serve --open",
    "build": "vue-cli-service build --target lib --name el-tree-select --dest dist src/components/index.js",
    "test:build": "vue-cli-service storybook:build -c config/storybook && rmdir /s/q docs\\storybook-static && move storybook-static docs",
    "test:dev": "vue-cli-service storybook:serve -p 1000 -c config/storybook",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "element-ui": "2.13.2",
    "core-js": "2.6.11",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.0 || ^5.0.0",
    "@storybook/addon-knobs": "^4.1.0 || ^5.0.0",
    "@storybook/addon-links": "^4.1.0 || ^5.0.0",
    "@storybook/addon-notes": "^4.1.0 || ^5.0.0",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-template-compiler": "^2.5.21"
  },
  "main": "dist/el-tree-select.umd.min.js",
  "files": [
    "dist"
  ]
}
